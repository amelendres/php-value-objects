{
    "name": "appto/php-value-objects",
    "description": "PHP value objects",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Appto\\": "src/Appto/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Unit\\": "tests/Unit/"
        }
    },
    "require": {
        "php": "^7.2.5",
        "ramsey/uuid": "^3.9",
        "symfony/intl": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.25",
        "fzaninotto/faker": "^1.9"
    },
    "scripts": {
        "post-install-cmd": [
            "./etc/hooks/install-hooks.sh"
        ],
        "phpunit": "vendor/bin/phpunit --exclude-group='disabled'",
        "phpstan": "vendor/bin/phpstan analyse -l 5 -c etc/phpstan/phpstan.neon src --quiet",
        "test": ["@phpstan", "@phpunit"]
    },
    "authors": [
        {
            "name": "Al Fred Do",
            "email": "alfredo.melendres@gmail.com"
        }
    ]
}
